            <string>/// Changes speed and sets movememnt direction to right.

// If puzzle has started
if(obj_textBoxLevelMaster.puzzle3GameStart == true)
{
    // If the player's current speed is less than the max speed (4), increment the speed by 0.15.
    if(speed &lt; maxPlayerSpeed)
    {
        speed +=0.15;
    }

    // If the player has collided with an invertDirection object reverse the movement direction.
    // Else set movement to the right.
    if(obj_commandDropper.invertDirection == true)
    {
